{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/ingest",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/analyze-patterns-v2",
      "schedule": "0 7 * * *"
    },
    {
      "path": "/api/cron/weekly-digest",
      "schedule": "0 14 * * 1"
    }
  ]
}
