{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/ingest",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/analyze-patterns",
      "schedule": "0 */6 * * *"
    }
  ]
}
